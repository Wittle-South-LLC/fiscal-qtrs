{
  "name": "fiscal-qtrs",
  "version": "0.1.0",
  "description": "Provide string representations of calendar & fiscal quarter based on a date",
  "main": "dist/cli.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "test": "NODE_ENV=test mocha --require @babel/register tests",
    "cover": "NODE_ENV=test nyc _mocha --require @babel/register tests",
    "gen": "babel-node src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Wittle-South-LLC/fiscal-qtrs.git"
  },
  "author": "Eric Wittle",
  "license": "MIT",
  "keywords": [
    "Fiscal",
    "Quarter",
    "Calendar"
  ],
  "bugs": {
    "url": "https://github.com/Wittle-South-LLC/fiscal-qtrs/issues"
  },
  "homepage": "https://github.com/Wittle-South-LLC/fiscal-qtrs#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-plugin-istanbul": "^5.1.4",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "exclude": [
      "**/test-*.js",
      "tests/**"
    ],
    "require": [
      "@babel/register"
    ],
    "extension": [
      ".jsx",
      ".js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {},
  "publishConfig":{
    "registry": "http://npm.wittlesouth.local"
  }
}
